start Machine

Machine ::= [StateMachine] "start" \start:State^ "current" current:State^   states:State*

State ::= [State] "state" name:sym Enter? Leave? out:Transition*
Enter ::= "enter" enter:str
Leave ::= "leave" leave:str

Transition ::= [Transition] "on" event:str Action? "go" to:State^
Action ::= "do" action:str
