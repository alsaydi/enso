
start Ledger

Ledger ::= [Ledger] accounts:Account* transactions:Transaction* 

Account ::= [Account] Type name:sym

Type ::= "asset"     {asset = true}
     |   "liability" {asset = false}

Transaction ::= [Transaction] "transaction" name:sym Proration
                   "{" transfers:Transfer* "}"

Transfer ::= [Transfer] account:Account^ ":" amount:int

Proration ::= "(" proration:int ")"
	   |  {proration = 1}



