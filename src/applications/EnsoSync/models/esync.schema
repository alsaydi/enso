	primitive \str
class Domain
  ! sources: Source *
  ! rules: Rule *
end

class Source
  # name: str
  ! basedir: Node ?
  path: str
end
class Node
  # name: str
end
class File < Node
  checksum: str ?
end
class Dir < Node
  ! nodes: Node *
end

class Rule
  # name: str
end
class SyncRule < Rule
  s1: Source
  s2: Source
  resolver: str
end
class CopyRule < Rule
  from: Source
  to: Source
end