start Domain

Domain ::= [Domain] "domain" sources:Source* "rules" rules:SyncRule* "end"

Source ::= [Source] "source" name:sym path:str basedir:Node ? "end"
Node ::= Dir | File
Dir ::= [Dir] name:sym ":(" nodes:Node * ")"
File ::= [File] name:sym "["checksum:str"]"

Rule ::= SyncRule
SyncRule ::= [SyncRule] name:sym ":" "synchronize" s1:Source^ "and" s2:Source^ 
