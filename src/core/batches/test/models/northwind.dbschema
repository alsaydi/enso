primitive int
primitive str
primitive real
primitive bool

class Category table=Categories
    #CategoryID: int
    CategoryName: str
    Description: str
    Picture: str
    Products: Product*
end

class Customer table=Customers
	#CustomerID: str
	CompanyName: str
	ContactName: str
	Country: str
	Orders: Order*
	Region: str
end

class Northwind table=Northwind
	Categories: Category*
	Products: Product*
	Suppliers: Supplier*
	Customers: Customer*
	Orders: Order*
end

class Order_Details table=Order_Details
	Order: Order / Order.OrderDetails
	Product: Product
	UnitPrice: real
	Quantity: int
	Discount: real
end

class Order table=Orders
	#OrderID: int
	OrderDate: int
    RequiredDate: int
    ShippedDate: int
	Customer: Customer / Customer.Orders
	OrderDetails: Order_Details*
    Freight: int
end

class Product table=Products
    #ProductID: int
    ProductName: str
    QuantityPerUnit: str
    UnitPrice: real
    UnitsInStock: int
    UnitsOnOrder: int
    ReorderLevel: int
    Discontinued: bool
    Category: Category / Category.Products
    Supplier: Supplier / Supplier.Products
    OrderDetails: Order_Details*
end

class Supplier table=Suppliers
	SupplierID: int
	CompanyName: str
	ContactName: str
	ContactTitle: str
    Address: str
    City: str
    Region: str
    PostalCode: str
    Country: str
    Phone: str
    Fax: str
    HomePage: str
    Products: Product*
end