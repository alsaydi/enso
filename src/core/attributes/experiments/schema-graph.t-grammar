


Graph ::= [Schema] Graph {nodes: Node*; edges: Edge*}
            @"@types = nodes"

Node ::= [Class] Node {name: str; shape: "box"}
      	    @"@name = name"

      |  [Primitive] Node {name: str; shape: "plain"} 
      	    @"@name = name"

Edge ::= [Field] Edge {from: Node^, to: Node^}
             @"@owner=from; @type=to"