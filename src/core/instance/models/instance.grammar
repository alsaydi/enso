
grammar InstanceGrammar

start Forest

Forest ::= [Dump] creates:Create*

Create ::=  [Create] name:sym  "{" contents:{Content ";"}* "}"

Content ::= [Field] name:sym ":" value:Value
         |  [Code] "@" code:str

Value ::= Create
       |  [Prim] "(" kind:sym ")" value:str
       |  [Ref] name:sym "^"
       |  [List] "[" elements:{Value ","}* "]"