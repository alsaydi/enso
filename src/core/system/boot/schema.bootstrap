primitive str
primitive int
primitive bool
primitive real

class Schema
  types: Type*
end

class Type
# name: str
  schema: Schema
end

class Primitive < Type
end

class Class < Type
  supers: Class*
  subclasses: Class*
! defined_fields: Field*
end

class Field
# name: str
  owner: Class
  type: Type
  optional: bool
  many: bool
  key: bool
  auto: bool
  inverse: Field?
  computed: Expr?
  traversal: bool
end

class Expr end
