primitive str
primitive int
primitive bool

class Expr end

class EBinOp < Expr
  op:str
  e1!Expr
  e2!Expr
end

class EUnOp < Expr
  op:str
  e!Expr
end

class EListComp < Expr
  op:str
  var:str
  list!Expr
  e!Expr
end

class EConst < Expr end
class EStrConst < EConst
  val:str
end
class EIntConst < EConst
  val:int
end
class EBoolConst < EConst
  val:bool
end
