interpret School

do Render (obj)

School (courses:Course*) :
    "puts 'Earnings reports for '+obj.name
    puts '---------------------------------------'
    obj.courses.each {|c|Render(c)}"

Course (cname:str, students:Student*, duration:int, cost:int):
    "puts '\nCourse: #{obj.cname}'
    obj.students.each {|s|Render(s)}
    puts 'Cost to run = $#{obj.duration*obj.cost}'"


Student (sname:str, fees:int):
    "puts ' #{obj.sname} ($#{obj.fees})'"
