interpret Expression

do render (expr)

EBinOp (e1:Expr, op:str, e2:Expr):
  "'#{e1} #{op} #{e2}'"

EUnOp (op:str, e:Expr):
  "'#{op} #{e}'"

EField (e:Expr, fname:str):
  "'#{e}.#{fname}'"

EVar (name:str):
  "'#{name}'"

EListComp (op:str, var:str, list:Expr, expr:Expr):
  "'#{op} #{var} in #{list} do { #{expr} }'"

EStrConst (val:int):
  "'#{val}'"

EIntConst (val:int):
  "'#{val}'"

EBoolConst (val:int):
  "'#{val}'"
